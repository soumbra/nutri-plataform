# Nutri Platform - Production Environment Variables
# Configure estas variáveis no Railway/Render

# Database (será fornecido pelo Railway/Render)
DATABASE_URL="postgresql://username:password@host:port/database"

# JWT - GERAR NOVO SECRET PARA PRODUÇÃO
JWT_SECRET="generate-new-super-secret-jwt-key-for-production"

# Next Auth
NEXTAUTH_SECRET="generate-new-nextauth-secret-for-production"
NEXTAUTH_URL="https://your-app-name.vercel.app"

# API - URL do backend em produção
API_URL="https://your-backend.railway.app"

# Port (Railway usa PORT automático)
PORT=3001

# Node Environment
NODE_ENV="production"